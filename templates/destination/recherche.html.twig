{% block filtre %}

  
{{form_start(form)}}
<div class="collapse" id="collapseExample">

<section  class="travel-box" style="margin-top:130px">

  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        <div class="single-travel-boxes">
          <div id="desc-tabs" class="desc-tabs">
            <div class="tab-content">
               <div role="tabpanel" class="tab-pane active fade in" id="tours">
                 <div class="tab-para">
                   
                   <div class="form-row">
                     <div class="form-group col-md-4">
                          {{ form_row(form.name , {attr: {class: 'form-control'}}) }}
                     </div>
                     <div class="form-group col-md-4">
                          {{ form_row(form.prix , {attr: {class: 'form-control'}}) }}
                          
                     </div>
                     <div class="form-group col-md-4">
                         {{ form_row(form.gouvernorat , {attr: {class: 'form-control'}}) }}
                     </div>
                    </div>
                  

              <div class="row">
                
                <div class="clo-sm-7">
                  <div class="about-btn    pull-right">
                    <button  class="about-view travel-btn">
                      Filtrer	
                    </button><!--/.travel-btn-->
                  </div><!--/.about-btn-->
                </div><!--/.col-->

              </div><!--/.row-->

            </div><!--/.tab-para-->

          </div><!--/.tabpannel-->

         
   
        </div><!--/.tab content-->
      </div><!--/.desc-tabs-->
        </div><!--/.single-travel-box-->
      </div><!--/.col-->
    </div><!--/.row-->
  </div><!--/.container-->

</section><!--/.travel-box-->
<!--travel-box end--> 
</div>
{{form_end(form)}}

{% endblock %}